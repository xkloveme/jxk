      "import": "./lib/index.es.js",
      "require": "./lib/index.cjs.js"
    }
  },
  "main": "./lib/index.cjs.js",
  "module": "./lib/index.es.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "gen": "node generate.js",
    "docs": "rimraf docs & jsdoc -c jsdoc.json",
